echo -e "Test 1:\n******************************************************"

curl -I -0 -X GET "http://127.0.0.1:12345/index.html"

echo -e "Test 2: \n******************************************************"
curl -I -0 -X GET "http://127.0.0.1:12345/index_.html"

echo -e "Test 3: \n******************************************************"
curl -I -0 -X GET "http://127.0.0.1:12345/index.html?x=1&y=22&z=333"

echo -e "Test 4: \n******************************************************"
curl -0 -X GET "http://127.0.0.1:12345/index.html"

echo -e "\n\nTest 5: \n******************************************************"
curl -0 -X GET "http://127.0.0.1:12345/index_.html"

echo -e "\n\nTest 6: \n******************************************************"
curl -0 -X GET "http://127.0.0.1:12345/index.html?x=1&y=22&z=333"